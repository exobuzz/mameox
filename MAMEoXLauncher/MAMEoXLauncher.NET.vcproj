			ConfigurationType="1"
			CharacterSet="2">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="0"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories=".\Includes;..\MAMEoX\Includes;..\MAME\src;..\;&quot;$(XDK)\Samples\XBox\Common\Include&quot;;&quot;..\libsmb++\src&quot;"
				PreprocessorDefinitions="_DEBUG;_XBOX"
				MinimalRebuild="TRUE"
				BasicRuntimeChecks="3"
				RuntimeLibrary="5"
				BufferSecurityCheck="TRUE"
				EnableFunctionLevelLinking="TRUE"
				UsePrecompiledHeader="0"
				WarningLevel="3"
				Detect64BitPortabilityProblems="FALSE"
				DebugInformationFormat="4"
				ForcedIncludeFiles="VCPPMame.h"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalDependencies="xapilibd.lib d3d8d.lib d3dx8d.lib xgraphicsd.lib dsoundd.lib dmusicd.lib xnetd.lib xboxkrnl.lib libsmbd.lib"
				OutputFile="../MAMEoX/Debug/MAMEoXLauncher.exe"
				LinkIncremental="2"
				AdditionalLibraryDirectories="&quot;..\libsmb++\Debug&quot;"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile="$(OutDir)/$(ProjectName).pdb"
				SubSystem="2"
				OptimizeForWindows98="1"
				TargetMachine="1"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="XboxDeploymentTool"
				RemotePath="xe:\MAMETEST\default.xbe"
				AdditionalFiles="xe:\MAMETEST\Media=..\Media"/>
			<Tool
				Name="XboxImageTool"
				AdditionalOptions="/testid:0x4D414D45"
				FileName="$(TargetDir)default.xbe"
				StackSize="65536"
				IncludeDebugInfo="TRUE"
				LimitAvailableMemoryTo64MB="TRUE"
				FormatUD="TRUE"
				NoLibWarn="TRUE"
				TitleName="&quot;MAMEoX Launcher [DBG]&quot;"/>
		</Configuration>
		<Configuration
			Name="Release|Xbox"
			OutputDirectory="Release"
			IntermediateDirectory="Release"
			ConfigurationType="1"
			CharacterSet="2">
			<Tool
				Name="VCCLCompilerTool"
				AdditionalOptions="/GL"
				Optimization="2"
				GlobalOptimizations="TRUE"
				InlineFunctionExpansion="2"
				EnableIntrinsicFunctions="TRUE"
				FavorSizeOrSpeed="1"
				OmitFramePointers="TRUE"
