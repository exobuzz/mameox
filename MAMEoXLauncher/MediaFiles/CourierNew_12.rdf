// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         ..\Includes\CourierNew_12.h
out_packedresource ..\..\Media\CourierNew_12.xpr
out_error          CourierNew_12.err


Texture Font
{
   Source      CourierNew_12.tga
   Format      D3DFMT_A4R4G4B4
   Levels      1
}

UserData FontData
{
   DataFile CourierNew_12.abc
}

